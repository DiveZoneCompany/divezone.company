{
    "overrides": [
        {
            "files": [
                "*.html"
            ],
            "options": {
                "parser": "go-template"
            }
        },
        {
            "files": [
                "*.html",
                "*.css",
                ".scss"
            ],
            "options": {
                "singleQuote": false
            }
        }
    ],
    "singleQuote": true,
    "printWidth": 120,
    "singleAttributePerLine": false,
    "bracketSameLine": true
}