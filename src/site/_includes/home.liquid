---
layout: base
eleventyImport:
  collections: ["brandsSorted"]
---

{{ content }}
<main>
  {{ content }}
</main>
<div class="brands">
  {%- for brand in collections.brandsSorted -%}
    <div
      class="brand brand-{{ brand.data.page.fileSlug }}"
      style="background-image: url('/images/{{ brand.data._image }}')">
      {{ brand.content }}
    </div>
  {%- endfor -%}
</div>